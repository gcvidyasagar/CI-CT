pipeline  {
    agent any
    stages {
        stage('checkout') {
            steps {
                script {
                sh """#!/bin/bash
                PATH="${HOME}/.bin:${PATH}"
                """
                }
    checkout changelog: true, poll: false, scm: [
    $class: 'RepoScm',
    currentBranch: true,
    forceSync: true,
    noTags: true,
    manifestBranch: 'main',
    manifestRepositoryUrl: 'git@github.com:gcvidyasagar/manifest.git',
    manifestFile: 'test.xml',
    quiet: true
    ]
  }
}
}
}

pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                script {
                    sh """#!/bin/bash
                    PATH=~/bin:$PATH
                    """
                }
                checkout changelog: true, poll: false, scm: [
                $class: 'RepoScm',
                currentBranch: true,
                forceSync: true,
                noTags: true,
                manifestBranch: 'main',
                manifestRepositoryUrl: 'git@github.com:gcvidyasagar/manifest.git',
                manifestFile: 'test.xml',
                quiet: true
                ]
        }
        }
    }
}
